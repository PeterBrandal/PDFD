{
    "manifest_version": 3,
    "name": "PDF Dictionary",
    "version": "1.0",
    "description": "A Chrome extension that provides a dictionary for PDF files.",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    } 
}